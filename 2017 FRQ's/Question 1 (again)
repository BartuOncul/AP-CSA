public Digits(int num)
{
    digitList = new ArrayList<Integer>();

    while(num == 0)
    {
        digitList.add(0);
    }

    while(num >0)
    {
        digitList.add(0, num%10);
        num = num/10;
    }

    
}

B)
public boolean isStrictlyIncreaing()
{
    for(int i=1; i<digitList.size(); i++)
    {
        if(digitList.get(i) <= digitList.get(i-1))
        {
            return false;
        }
    }
    return true;
}
