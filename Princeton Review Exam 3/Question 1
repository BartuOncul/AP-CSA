A)
public boolean findEmloyeeForChild(Child c)
{
    for(int i=0; i<children.size(); i++)
    {
        if((children.get(i).getAge()<=maxRatio && c.get(i).canTeach(int age))
        {
            return true;
        }
    }
    return false;
}
B)
public boolean runDayCare()
{
    for(int i=0; i<children.size(); i++)
    {
        if(!(assignChild(c)))
        {
            return false;
        }
    }
    return true;
}
