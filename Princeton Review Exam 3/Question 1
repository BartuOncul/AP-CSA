A)
public boolean findEmloyeeForChild(Child c)
{    
    for(int i=0; i<children.size(); i++)
    {
        if((children.get(i).getAge()<maxRatio && c.canTeach(age))
        {
            return true;
        }
    }
    return false;
}
B)
public boolean runDayCare()
{
    for(int i=0; i<children.size(); i++)
    {
        if(!(findEmloyeeForChild(c)))
        {
            child.get(i).assignChild(c);
            return false;
        }
    }
    return true;
}
C)
public boolean addChild(Child c)
{
    if(findEmloyeeForChild(c) == true)
    {
        children.add(c);
        return true;
    }
    return false;
}
