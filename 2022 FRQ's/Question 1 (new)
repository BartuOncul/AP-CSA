public int getScore()
{
    int score = 0;
    if(levelOne.goalReached())
    {  
        if(levelTwo.goalReached())
        {
            if(levelThree.goalReached())
            {
                score+=levelThree;
            }
            score+=levelTwo;
        }
        score+=levelOne;
    }
    if(isBonus())
    {
        score*=3;
    }
}

